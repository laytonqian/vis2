{
    "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
    "width": 800,
    "height": 500,
    "title": "Renewable Energy Consumption 1965 - 2018",
    "data": {
      "url": "https://raw.githubusercontent.com/laytonqian/vis2/master/data/renewable_energy_population_final_v5.csv"
    },
    
    "layer": [
        {   "mark": {
            "type": "line"

            },
            "transform": [
                {"filter": "datum.Year == Year_Selection"},
                {"filter": "datum.Renewable_Source == Renewable_Source_Selection"}
            ],  
            "encoding": {
            "x": {
                "field": "Year",
                "timeUnit": "year",
                "title": "Year"
            },
            "y": {
                "aggregate": "sum", "field": "Terawatt_Hours",
                "type": "quantitative",
                "title": "Consumption in Terawatt Hours",
                "scale": {"type": "symlog"}
                
            }
            ,
            "color": {
                "type": "nominal",
                "field": "Renewable_Source",
                "scale": {"range": ["#e41a1c","#377eb8","#4daf4a","#984ea3","#ff7f00"]}
            },
            "tooltip": [
                {"aggregate": "sum", "field": "Terawatt_Hours", "type": "quantitative", "title": "Terawatt Hours"},
                {"field": "Renewable_Source", "type": "nominal"},
                {"field": "Year", "timeUnit": "year", "title": "Year"}
      
            ]
         
            }
        }
          
      
        
    ],
    
    "config": {}
  }
