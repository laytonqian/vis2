{
    "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
    "width": 800,
    "height": 500,
    "title": "Renewable Energy Consumption",
    "data": {
      "url": "https://raw.githubusercontent.com/laytonqian/vis2/master/data/renewable_energy_population_final_v3.csv"
    },
    "layer": [
        {   "mark": {
            "type": "line"

            },
            "encoding": {
            "x": {
                "field": "Year",
                "timeUnit": "year",
                "title": "Year"
            },
            "y": {
                "aggregate": "sum", "field": "Terawatt Hours",
                "type": "quantitative",
                "title": "Terawatt Hours",
                "scale": {"type": "sqrt"}
                
            }
            ,
            "color": {
                "type": "nominal",
                "field": "Renewable Source"
            }
         
            }
        }
          
      
        
    ],
    
    "config": {}
  }
